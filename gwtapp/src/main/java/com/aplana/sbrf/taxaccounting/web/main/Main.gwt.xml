	<inherits name='com.google.gwt.uibinder.UiBinder' />
	<inherits name='com.google.gwt.inject.Inject' />

	<!-- Main -->
	<inherits name="com.aplana.sbrf.taxaccounting.web.main.entry.MainEntry" />
	<inherits name="com.aplana.sbrf.taxaccounting.web.main.api.MainApi" />
	<inherits name="com.aplana.sbrf.taxaccounting.web.main.page.MainPage" />

	<!-- Control -->
	<inherits name="com.aplana.sbrf.taxaccounting.web.widget.Widget" />

	<!-- Module -->
	<inherits name="com.aplana.sbrf.taxaccounting.web.module.Module" />

	<!-- Theme -->
	<!--<inherits name='com.google.gwt.user.theme.standard.Standard' />-->
    <inherits name="com.aplana.sbrf.taxaccounting.theme.sberbank.Sberbank"/>

	<!-- Browser -->
	<set-property name="user.agent" value="gecko1_8,safari,ie8,ie9"/>
