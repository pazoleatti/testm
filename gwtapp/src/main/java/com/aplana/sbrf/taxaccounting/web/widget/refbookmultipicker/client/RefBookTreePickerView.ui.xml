<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:multiselecttree="urn:import:com.aplana.sbrf.taxaccounting.web.widget.multiselecttree">

    <ui:style>
        .tableWrapper {
            background-color: #ffffff;
        }
    </ui:style>

    <g:ScrollPanel width="100%" height="100%" addStyleNames="{style.tableWrapper}">
        <multiselecttree:LazyTree ui:field="tree" width="100%" height="100%"/>
    </g:ScrollPanel>

</ui:UiBinder>