    <inherits name="com.aplana.sbrf.taxaccounting.web.module.Module" />

    <set-configuration-property name="gin.ginjector.modules"
                                value="com.aplana.sbrf.taxaccounting.web.main.entry.client"/>

	<!-- This is required for Gwt-Platform Proxy's generator -->
	<!-- 
	<define-configuration-property name="gin.ginjector" is-multi-valued="false" />
	<set-configuration-property name="gin.ginjector" value="com.aplana.sbrf.taxaccounting.web.main.entry.client.ClientGinjector" />
	 -->
	 <set-configuration-property name="gin.ginjector.modules"
                                value="com.aplana.sbrf.taxaccounting.web.main.entry.client.ClientModule"/>


</module>
