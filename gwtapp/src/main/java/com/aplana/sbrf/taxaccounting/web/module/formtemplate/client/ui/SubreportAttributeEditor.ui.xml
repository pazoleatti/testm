<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <g:HorizontalPanel height="30px">
            <g:Label width="150px">Псевдоним</g:Label>
            <g:TextBox width="200%" ui:field="alias"/>
        </g:HorizontalPanel>

        <g:HorizontalPanel height="30px">
            <g:Label width="150px">Наименование специфического отчета</g:Label>
            <g:TextBox width="200%" ui:field="name"/>
        </g:HorizontalPanel>
    </g:HTMLPanel>
</ui:UiBinder>