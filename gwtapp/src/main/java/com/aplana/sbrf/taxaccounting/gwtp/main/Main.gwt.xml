<module rename-to="Main">

  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.google.gwt.uibinder.UiBinder' />
  <inherits name='com.google.gwt.inject.Inject' />
  
  <!-- Main -->
  <inherits name="com.aplana.sbrf.taxaccounting.gwtp.main.api.MainApi" />
  <inherits name="com.aplana.sbrf.taxaccounting.gwtp.main.entry.MainEntry" />
  <inherits name="com.aplana.sbrf.taxaccounting.gwtp.main.control.MainControl" />
  <inherits name="com.aplana.sbrf.taxaccounting.gwtp.main.page.MainPage"/>
  
  <!-- Module -->
  <inherits name="com.aplana.sbrf.taxaccounting.gwtp.module.Module" />
  
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  
 
  
