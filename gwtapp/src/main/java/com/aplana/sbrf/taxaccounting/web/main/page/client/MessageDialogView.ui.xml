<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:log="urn:import:com.aplana.sbrf.taxaccounting.web.widget.log">

	<g:DialogBox ui:field="dialogBox" modal="false">
		<g:VerticalPanel ui:field="htmlPanel">
			<g:Label ui:field="message" />
			<log:LogEntriesWidget ui:field="logEntriesView" />
			<log:ThrowableWidget ui:field="throwableView" />
            <g:cell horizontalAlignment="ALIGN_CENTER">
			    <g:Button width="50%" ui:field="okButton">Закрыть сообщение</g:Button>
            </g:cell>
		</g:VerticalPanel>
	</g:DialogBox>

</ui:UiBinder> 