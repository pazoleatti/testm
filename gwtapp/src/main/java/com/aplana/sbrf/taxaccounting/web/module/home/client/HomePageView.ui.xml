    <ui:style>
        .title {
            font-size: 26px;
            text-wrap: none;
            white-space: nowrap;
        }
    </ui:style>

	<g:HorizontalPanel width="100%" height="100%" >
        <g:cell horizontalAlignment="center" verticalAlignment="middle">
            <g:HTML addStyleNames="{style.title}">Автоматизированная система «Учет налогов»</g:HTML>
        </g:cell>

	</g:HorizontalPanel>

</ui:UiBinder> 