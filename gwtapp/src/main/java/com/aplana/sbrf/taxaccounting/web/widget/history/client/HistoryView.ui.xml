<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:style="urn:import:com.aplana.sbrf.taxaccounting.web.widget.style">

    <ui:style>
        .marginTop{
            margin-top: 3px;
        }
    </ui:style>

    <g:DialogBox glassEnabled="true" animationEnabled="true"
                 text="Информация по налоговой форме/декларации">
        <g:FlowPanel>
            <style:GenericDataGrid ui:field="logsTable" height="250px" width="1000px" />
            <style:Bar styleName="{style.marginTop}">
                <style:RightBar>
                    <g:Button text="Закрыть" ui:field="hideButton"/>
                </style:RightBar>
            </style:Bar>
        </g:FlowPanel>
    </g:DialogBox>

</ui:UiBinder>