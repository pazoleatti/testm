<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<g:DockLayoutPanel unit="PX" height="100%" width="100%">
        <g:north size="50">
            <g:FlowPanel>
                <g:Label text="Информация о данных по налоговой форме"/>
            </g:FlowPanel>
        </g:north>
        <g:center>
			<c:DataGrid ui:field="formDataTable" focus="true"/>
		</g:center>
		<g:south size="50">
            <g:FlowPanel>
			    <g:Button ui:field="cancelButton">Отмена</g:Button>
            </g:FlowPanel>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder>
