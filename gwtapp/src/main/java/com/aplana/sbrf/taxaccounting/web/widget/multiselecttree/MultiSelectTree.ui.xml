<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style src="MultiSelectTree.css"/>

    <g:VerticalPanel styleName="{style.msiPanel}">
        <g:cell height="100%" width="100%">
            <g:Label ui:field="label" styleName="{style.msiHeader}"/>
        </g:cell>
        <g:ScrollPanel ui:field="scrollPanel" styleName="{style.msiTreePanel}">
            <!--<g:HorizontalPanel styleName="{style.msiTreePanel}">-->
                <g:Tree ui:field="tree" styleName="{style.tree}"/>
            <!--</g:HorizontalPanel>-->
        </g:ScrollPanel>
    </g:VerticalPanel>

</ui:UiBinder>