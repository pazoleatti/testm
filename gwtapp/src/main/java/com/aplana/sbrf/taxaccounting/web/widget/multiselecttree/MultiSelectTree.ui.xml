<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style src="MultiSelectTree.css"/>

    <g:VerticalPanel styleName="{style.msiPanel}">
        <g:cell width="100%">
            <g:Label ui:field="label" styleName="{style.msiHeader}"/>
        </g:cell>
        <g:HorizontalPanel styleName="{style.msiCbAndTreePanel}">
            <g:cell height="100%">
                <g:VerticalPanel ui:field="cbPanel" styleName="{style.msiCheckBoxPanel}"/>
            </g:cell>
            <g:cell height="100%" width="100%">
                <g:Tree ui:field="tree" styleName="{style.tree}"/>
            </g:cell>
        </g:HorizontalPanel>
    </g:VerticalPanel>

</ui:UiBinder>