<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:style>
        @external .gwt-MenuBar-horizontal;
        .gwt-MenuBar-horizontal {
            background: none;
            border: none;
        }
        @external .gwt-MenuBar-horizontal .gwt-MenuItem;
        .gwt-MenuBar-horizontal .gwt-MenuItem {
            color: white;
        }
        @external .gwt-MenuBar .gwt-MenuItem-selected;
        .gwt-MenuBar .gwt-MenuItem-selected {
            background: none;
            border-bottom: 1px dotted #6A8EA4;
            color: #3964C2 !important;
            cursor: pointer;
        }

        @external .gwt-MenuBar-vertical .gwt-MenuItem;
        .gwt-MenuBar-vertical .gwt-MenuItem {
            background: repeat-x scroll center bottom #76BD43;
        }

    </ui:style>

	<g:FlowPanel ui:field="panel"/>

</ui:UiBinder> 