<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">

<module>
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.uibinder.UiBinder' />
	<inherits name='com.google.gwt.inject.Inject' />

	<!-- TaxAccounting Modules -->
	<inherits name="com.aplana.sbrf.taxaccounting.web.module.formtemplate.FormTemplate" />
    <inherits name="com.aplana.sbrf.taxaccounting.web.module.declarationtemplate.DeclarationTemplate" />
    <inherits name="com.aplana.sbrf.taxaccounting.web.module.declarationdata.DeclarationData" />
	<inherits name="com.aplana.sbrf.taxaccounting.web.module.home.HomeModule" />
	<inherits name="com.aplana.sbrf.taxaccounting.web.module.formdatalist.FormDataList" />
	<inherits name="com.aplana.sbrf.taxaccounting.web.module.formdata.FormData" />
	<inherits name="com.aplana.sbrf.taxaccounting.web.module.error.ErrorModule" />
    <inherits name="com.aplana.sbrf.taxaccounting.web.module.declarationlist.DeclarationList"/>
    <inherits name="com.aplana.sbrf.taxaccounting.web.module.userlist.UserList"/>
    <inherits name="com.aplana.sbrf.taxaccounting.web.module.formdataimport.FormDataImport"/>
    <inherits name="com.aplana.sbrf.taxaccounting.web.module.audit.Audit"/>
    <inherits name="com.aplana.sbrf.taxaccounting.web.module.periods.Periods"/>

    <replace-with class="com.google.gwt.user.cellview.client.CustomCellBasedWidgetImplTrident">
        <when-type-is class="com.google.gwt.user.cellview.client.CellBasedWidgetImpl"/>
        <any>
            <when-property-is name="user.agent" value="ie6"/>
            <when-property-is name="user.agent" value="ie8"/>
        </any>
    </replace-with>
</module>
