<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:G="urn:import:com.google.gwt.user.client.ui">

	<g:SimplePanel width="200px">
		<g:HorizontalPanel verticalAlignment="middle" height="100%" ui:field="panel">
			<g:cell width="100%">
				<g:TextBox enabled="false" ui:field="selected" width="100%" />
			</g:cell>
			<g:cell>
				<g:Button ui:field="selectButton" text="..." width="25px" />
			</g:cell>
			<g:cell>
				<g:Button text="X" ui:field="clearButton" width="25px" />
			</g:cell>
		</g:HorizontalPanel>
	</g:SimplePanel>

</ui:UiBinder>
