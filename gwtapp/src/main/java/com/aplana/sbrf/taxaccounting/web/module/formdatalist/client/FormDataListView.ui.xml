	xmlns:style="urn:import:com.aplana.sbrf.taxaccounting.web.widget.style">

	<g:VerticalPanel width="100%">

		<g:Label text="Информация о данных по налоговой форме" height="20px" />
		<g:cell horizontalAlignment="ALIGN_CENTER" width="100%">
			<g:Label ui:field="taxTypeLabel" />
		</g:cell>
		<g:HorizontalPanel width="100%" ui:field="filterContentPanel" />

		<style:Bar>
			<style:LeftBar>
				<g:Button ui:field="create">Создать</g:Button>
				<g:Button ui:field="apply">Применить</g:Button>
			</style:LeftBar>
		</style:Bar>

		<g:HorizontalPanel>

		</g:HorizontalPanel>
		<cell:CellTable ui:field="formDataTable" width="100%" />

	</g:VerticalPanel>

</ui:UiBinder> 