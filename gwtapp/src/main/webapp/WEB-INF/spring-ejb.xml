<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:jee="http://www.springframework.org/schema/jee"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-3.1.xsd">

    <jee:remote-slsb id="messageService"
                     jndi-name="ejblocal:${releaseName}/migration-ejb.jar/MigrationBean#com.aplana.sbrf.taxaccounting.service.MessageServiceLocal"
                     business-interface="com.aplana.sbrf.taxaccounting.service.MessageService"/>

    <jee:remote-slsb
            id="taskManager"
            jndi-name="ejblocal:${releaseName}/scheduler-core.jar/TaskManagerBean#com.aplana.sbrf.taxaccounting.scheduler.api.manager.TaskManagerLocal"
            business-interface="com.aplana.sbrf.taxaccounting.scheduler.api.manager.TaskManager"
            lookup-home-on-startup="true"
            />
</beans>
