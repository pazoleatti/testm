<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aplana.sbrf.taxaccounting.service.FormTemplateService"/>
    </bean>
    <bean class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aplana.sbrf.taxaccounting.web.main.api.server.SecurityService"/>
    </bean>
    <bean class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aplana.sbrf.taxaccounting.service.FormTemplateImpexService"/>
    </bean>
    <bean class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aplana.sbrf.taxaccounting.service.LogEntryService"/>
    </bean>
    <bean class="org.mockito.Mockito" factory-method="mock" name="formTemplateMainOperatingService">
        <constructor-arg value="com.aplana.sbrf.taxaccounting.service.MainOperatingService"/>
    </bean>
    <bean class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aplana.sbrf.taxaccounting.service.TAUserService"/>
    </bean>

    <bean id="formTemplateController" class="com.aplana.sbrf.taxaccounting.web.mvc.FormTemplateController"/>

</beans>