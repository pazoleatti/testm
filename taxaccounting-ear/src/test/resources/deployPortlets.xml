<request xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" type="update" create-oids="true" xsi:noNamespaceSchemaLocation="PortalConfig_7.0.0.xsd">
	<portal action="locate">
		<!-- web-app action="delete" uid="aplana.sbrf.taxaccounting.portlets.webmod" /-->
		<web-app action="update" active="true"
			uid="aplana.sbrf.taxaccounting.portlets.webmod" predeployed="true">
			<url>file:///$installedApps$/taxaccounting.ear/portlets.war</url>
			<context-root>/taxaccounting/portlets</context-root>
			<display-name>Tax Accounting Portlets</display-name>
			<portlet-app action="update" active="true" name="portlets.war"
				uid="com.aplana.sbrf.taxaccounting.portlets.portletapp">
				<access-control>
					<role actionset="Privileged User" update="set">
						<mapping subjectid="all authenticated portal users" subjecttype="user_group" update="set" />
					</role>
				</access-control>
				<portlet action="update" active="true" name="form" provided="false" />
				<portlet action="update" active="true" name="formData" provided="false" />
			</portlet-app>
		</web-app>
	</portal>
</request>