<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>false</fixedRows>
    <name>Расшифровка операций по реализации товаров (работ, услуг) на безвозмездной основе</name>
    <fullName>Расшифровка операций по реализации товаров (работ, услуг) на безвозмездной основе</fullName>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="autoNumerationColumn">
        <alias>rowNum</alias>
        <checking>false</checking>
        <columnType>AUTO</columnType>
        <name>№ п/п</name>
        <order>1</order>
        <width>5</width>
        <numerationType>CROSS</numerationType>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>operName</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>Наименование операции</name>
        <order>2</order>
        <width>11</width>
        <maxLength>1000</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>taxBase</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Налоговая база по данным бухгалтерского учета</name>
        <order>3</order>
        <width>11</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>taxRate</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>Ставка налога</name>
        <order>4</order>
        <width>5</width>
        <maxLength>3</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>taxSum</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Сумма налога</name>
        <order>5</order>
        <width>11</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>check</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Проверка (гр.6=гр.5/гр.4-гр.3)</name>
        <order>6</order>
        <width>11</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>comment</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>Пояснения гр.6</name>
        <order>7</order>
        <width>11</width>
        <maxLength>1000</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <styles>
        <alias>Редактируемая</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Заголовок</alias>
        <backColor>GREY</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-добавлено</alias>
        <backColor>PALE_GREEN</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-удалено</alias>
        <backColor>LIGHT_CORAL</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-без изменений</alias>
        <backColor>GREY</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-изменено</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Контрольные суммы</alias>
        <backColor>LIGHT_BROWN</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Автозаполняемая</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>DARK_GREEN</fontColor>
        <italic>false</italic>
    </styles>
</formTemplateContent>