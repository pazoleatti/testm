									<sch:assert test="
( count(СвНП/НПЮЛ/@КПП)!=0 and 
( ( (substring(СвНП/НПЮЛ/@КПП,5,2)=50) and @ПоМесту=213 ) 
or 
( (substring(СвНП/НПЮЛ/@КПП,5,2)=36) and @ПоМесту=250 ) 
or 
( ( (number(substring(СвНП/НПЮЛ/@КПП,5,2)='4T') ) or (number(substring(СвНП/НПЮЛ/@КПП,5,2)='5G') ) ) and @ПоМесту=227 )
or
( (substring(СвНП/НПЮЛ/@КПП,5,2)!=50) and (substring(СвНП/НПЮЛ/@КПП,5,2)!=36) and (number(substring(СвНП/НПЮЛ/@КПП,5,2)!='4T') )  and  (number(substring(СвНП/НПЮЛ/@КПП,5,2)!='5G') ) and (@ПоМесту=214 or @ПоМесту=215 or @ПоМесту=216 or @ПоМесту=231  or @ПоМесту=331) ) )
	) 
	or 
	(  count(СвНП/НПЮЛ/@КПП)=0  )
	">
										<usch:error code="0400400007" path="@ПоМесту" value-of-select="@ПоМесту"> Значение элемента Файл/Документ/ПоМесту не соответствует условию при значении элемента КПП, равном "<usch:value-of select="СвНП/НПЮЛ/@КПП"/>". </usch:error>
									</sch:assert>
