<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>false</fixedRows>
    <name>Сведения о льготируемых транспортных средствах, по которым уплачивается транспортный налог</name>
    <fullName>Сведения о льготируемых транспортных средствах, по которым уплачивается транспортный налог</fullName>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="autoNumerationColumn">
        <alias>rowNumber</alias>
        <checking>false</checking>
        <name>№ пп</name>
        <order>1</order>
        <width>5</width>
        <type>0</type>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>codeOKATO</alias>
        <checking>false</checking>
        <name>Код ОКТМО</name>
        <order>2</order>
        <width>7</width>
        <hierarchical>true</hierarchical>
        <nameAttributeId>841</nameAttributeId>
        <refBookAttributeId>840</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>identNumber</alias>
        <checking>false</checking>
        <name>Идентификационный номер</name>
        <order>3</order>
        <width>13</width>
        <maxLength>25</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>regNumber</alias>
        <checking>false</checking>
        <name>Регистрационный знак</name>
        <order>4</order>
        <width>9</width>
        <maxLength>9</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>powerVal</alias>
        <checking>false</checking>
        <name>Мощность (величина)</name>
        <order>5</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>baseUnit</alias>
        <checking>false</checking>
        <name>Мощность (ед. измерения)</name>
        <order>6</order>
        <width>8</width>
        <hierarchical>false</hierarchical>
        <nameAttributeId>58</nameAttributeId>
        <refBookAttributeId>57</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>taxBenefitCode</alias>
        <checking>false</checking>
        <name>Код налоговой льготы</name>
        <order>7</order>
        <width>5</width>
        <filter>CODE = '30200' OR CODE = '20210' OR CODE = '20220' OR CODE = '20230'</filter>
        <hierarchical>true</hierarchical>
        <nameAttributeId>16</nameAttributeId>
        <refBookAttributeId>15</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="dateColumn">
        <alias>benefitStartDate</alias>
        <checking>false</checking>
        <name>Дата начала</name>
        <order>8</order>
        <width>7</width>
        <formatId>0</formatId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="dateColumn">
        <alias>benefitEndDate</alias>
        <checking>false</checking>
        <name>Дата окончания</name>
        <order>9</order>
        <width>7</width>
        <formatId>0</formatId>
    </columns>
    <styles>
        <alias>Корректировка-изменено</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-без изменений</alias>
        <backColor>GREY</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-добавлено</alias>
        <backColor>PALE_GREEN</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-удалено</alias>
        <backColor>LIGHT_CORAL</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Автозаполняемая</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>DARK_GREEN</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Редактируемая</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
</formTemplateContent>