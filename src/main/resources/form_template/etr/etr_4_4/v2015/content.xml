<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>true</fixedRows>
    <monthly>false</monthly>
    <comparative>true</comparative>
    <accruing>true</accruing>
    <updating>false</updating>
    <name>Приложение 4-4. Отношение налоговых платежей к операционным доходам</name>
    <fullName>Приложение 4-4. Отношение налоговых платежей к операционным доходам</fullName>
    <header>Приложение 4-4</header>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>rowNum</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>№ строки</name>
        <order>1</order>
        <width>5</width>
        <maxLength>1</maxLength>
        <precision>0</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>taxName</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>Наименование налога</name>
        <order>2</order>
        <width>20</width>
        <maxLength>256</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>comparePeriod</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Период сравнения, тыс. руб.</name>
        <order>3</order>
        <width>10</width>
        <maxLength>22</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>currentPeriod</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Период, тыс. руб.</name>
        <order>4</order>
        <width>10</width>
        <maxLength>22</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>deltaRub</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Изменение за период (гр.4-гр.3), тыс.руб.</name>
        <order>5</order>
        <width>10</width>
        <maxLength>22</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>deltaPercent</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Изменение за период (гр.5/гр.3*100),%</name>
        <order>6</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <styles>
        <alias>Автозаполняемая</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>DARK_GREEN</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Редактируемая</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Контрольные суммы</alias>
        <backColor>LIGHT_BROWN</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-добавлено</alias>
        <backColor>PALE_GREEN</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-удалено</alias>
        <backColor>LIGHT_CORAL</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-без изменений</alias>
        <backColor>GREY</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-изменено</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Жирный</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
</formTemplateContent>