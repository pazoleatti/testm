<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>false</fixedRows>
    <name>Приложение 4-11. Статистика доначислений</name>
    <fullName>Приложение 4-11. Статистика доначислений</fullName>
    <header>Приложение 4-11</header>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="autoNumerationColumn">
        <alias>rowNum</alias>
        <checking>false</checking>
        <columnType>AUTO</columnType>
        <name>№ п/п</name>
        <order>1</order>
        <width>4</width>
        <numerationType>SERIAL</numerationType>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>name</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>Наименование сделки</name>
        <order>2</order>
        <width>30</width>
        <maxLength>500</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>sum1</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Сумма фактического дохода/расхода по нерыночным сделкам, тыс. руб. (налоговый учет)</name>
        <order>3</order>
        <width>10</width>
        <maxLength>22</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>sum2</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Сумма доначислений до рыночного уровня, тыс. руб.</name>
        <order>4</order>
        <width>10</width>
        <maxLength>22</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>level</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Уровень доначислений/ не учитываемых расходов (в % от факта)</name>
        <order>5</order>
        <width>10</width>
        <maxLength>7</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>taxBurden</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Налоговое бремя, тыс. руб.</name>
        <order>6</order>
        <width>10</width>
        <maxLength>22</maxLength>
        <precision>2</precision>
    </columns>
    <styles>
        <alias>Автозаполняемая</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>DARK_GREEN</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Редактируемая</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Контрольные суммы</alias>
        <backColor>LIGHT_BROWN</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-добавлено</alias>
        <backColor>PALE_GREEN</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-удалено</alias>
        <backColor>LIGHT_CORAL</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-без изменений</alias>
        <backColor>GREY</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-изменено</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
</formTemplateContent>