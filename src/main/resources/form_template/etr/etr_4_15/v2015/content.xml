<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>true</fixedRows>
    <monthly>false</monthly>
    <comparative>true</comparative>
    <accruing>true</accruing>
    <updating>false</updating>
    <name>Приложение 4-15. Анализ структуры налога на добавленную стоимость (НДС)</name>
    <fullName>Приложение 4-15. Анализ структуры налога на добавленную стоимость (НДС)</fullName>
    <header>Приложение 4-15</header>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>comparePeriod</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Период сравнения. НДС всего, тыс. руб.</name>
        <order>1</order>
        <width>15</width>
        <maxLength>22</maxLength>
        <precision>4</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>comparePeriodIgnore</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Период сравнения. В том числе НДС не учитываемый, тыс. руб.</name>
        <order>2</order>
        <width>15</width>
        <maxLength>22</maxLength>
        <precision>4</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>comparePeriodPercent</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Период сравнения. Доля НДС не учитываемый, %</name>
        <order>3</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>currentPeriod</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Период. НДС всего, тыс. руб.</name>
        <order>4</order>
        <width>15</width>
        <maxLength>22</maxLength>
        <precision>4</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>currentPeriodIgnore</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Период. В том числе НДС не учитываемый, тыс. руб.</name>
        <order>5</order>
        <width>15</width>
        <maxLength>22</maxLength>
        <precision>4</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>currentPeriodPercent</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Период. Доля НДС не учитываемый, %</name>
        <order>6</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>delta</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Изменения за период. НДС всего, тыс. руб.</name>
        <order>7</order>
        <width>10</width>
        <maxLength>22</maxLength>
        <precision>4</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>deltaIgnore</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Изменения за период. В том числе НДС не учитываемый, тыс. руб.</name>
        <order>8</order>
        <width>10</width>
        <maxLength>22</maxLength>
        <precision>4</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>deltaPercent</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Изменения за период. Доля НДС не учитываемый, %</name>
        <order>9</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <styles>
        <alias>Автозаполняемая</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>DARK_GREEN</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Редактируемая</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Контрольные суммы</alias>
        <backColor>LIGHT_BROWN</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
