<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>false</fixedRows>
    <monthly>false</monthly>
    <comparative>false</comparative>
    <accruing>false</accruing>
    <updating>false</updating>
    <name>Данные по непокрытым аккредитивам</name>
    <fullName>Информация о выданных непокрытых аккредитивах</fullName>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="autoNumerationColumn">
        <alias>rowNumber</alias>
        <checking>false</checking>
        <name>№ п/п</name>
        <order>1</order>
        <width>5</width>
        <numerationType>SERIAL</numerationType>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>productType</alias>
        <checking>false</checking>
        <name>Вид Продукта (гарантия / непокрытый аккредитив)</name>
        <order>2</order>
        <width>10</width>
        <maxLength>64</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>name</alias>
        <checking>false</checking>
        <name>Наименование контрагента и ОПФ</name>
        <order>3</order>
        <width>20</width>
        <maxLength>256</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>country</alias>
        <checking>false</checking>
        <columnType>REFBOOK</columnType>
        <name>Страна регистрации (местоположения)</name>
        <order>4</order>
        <width>9</width>
        <hierarchical>false</hierarchical>
        <nameAttributeId>53</nameAttributeId>
        <refBookAttributeId>53</refBookAttributeId>
        <searchEnabled>true</searchEnabled>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>innKio</alias>
        <checking>false</checking>
        <name>ИНН / КИО клиента</name>
        <order>5</order>
        <width>10</width>
        <maxLength>12</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="dateColumn">
        <alias>docDate</alias>
        <checking>false</checking>
        <name>Дата подписания Договора / ГенСоглашения</name>
        <order>6</order>
        <width>5</width>
        <formatId>0</formatId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>docNumber</alias>
        <checking>false</checking>
        <name>Номер обязательства (референс)</name>
        <order>7</order>
        <width>20</width>
        <maxLength>240</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="dateColumn">
        <alias>creditDate</alias>
        <checking>false</checking>
        <name>Дата выдачи обязательства</name>
        <order>8</order>
        <width>5</width>
        <formatId>0</formatId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="dateColumn">
        <alias>creditEndDate</alias>
        <checking>false</checking>
        <name>Дата окончания действия обязательства</name>
        <order>9</order>
        <width>5</width>
        <formatId>0</formatId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>sum</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Сумма обязательства (в ед. валюты)</name>
        <order>10</order>
        <width>10</width>
        <maxLength>14</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>currency</alias>
        <checking>false</checking>
        <name>Валюта выдачи</name>
        <order>11</order>
        <width>6</width>
        <hierarchical>false</hierarchical>
        <nameAttributeId>65</nameAttributeId>
        <refBookAttributeId>65</refBookAttributeId>
        <searchEnabled>true</searchEnabled>
        <versioned>true</versioned>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>period</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Срок в днях</name>
        <order>12</order>
        <width>8</width>
        <maxLength>4</maxLength>
        <precision>0</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>creditRating</alias>
        <checking>false</checking>
        <name>Кредитный рейтинг контрагента / класс кредитоспособности</name>
        <order>13</order>
        <width>6</width>
        <hierarchical>false</hierarchical>
        <nameAttributeId>6034</nameAttributeId>
        <refBookAttributeId>6034</refBookAttributeId>
        <searchEnabled>true</searchEnabled>
        <versioned>true</versioned>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>faceValueStr</alias>
        <checking>false</checking>
        <name>Номинальная ставка (в % или в абсолютном выражении) платы за выданный аккредитив / гарантию</name>
        <order>14</order>
        <width>20</width>
        <maxLength>255</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>faceValueNum</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>Номинальная ставка (в % годовых или в абсолютном выражении) плата за подтверждение платежа по аккредитиву</name>
        <order>15</order>
        <width>8</width>
        <maxLength>6</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>paymentSchedule</alias>
        <checking>false</checking>
        <name>График платежей по уплате комиссий</name>
        <order>16</order>
        <width>20</width>
        <maxLength>32</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>sign</alias>
        <checking>false</checking>
        <columnType>REFBOOK</columnType>
        <name>Наличие обеспечения / поручительства (Да / Нет)</name>
        <order>17</order>
        <width>6</width>
        <hierarchical>false</hierarchical>
        <nameAttributeId>250</nameAttributeId>
        <refBookAttributeId>250</refBookAttributeId>
        <searchEnabled>true</searchEnabled>
    </columns>

    <styles>
        <alias>Корректировка-изменено</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-без изменений</alias>
        <backColor>GREY</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-добавлено</alias>
        <backColor>PALE_GREEN</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-удалено</alias>
        <backColor>LIGHT_CORAL</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Редактируемая</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Автозаполняемая</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>DARK_GREEN</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Контрольные суммы</alias>
        <backColor>LIGHT_BROWN</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
</formTemplateContent>