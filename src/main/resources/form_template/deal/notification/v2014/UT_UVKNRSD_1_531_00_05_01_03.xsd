                                                                        </sch:assert>
                                                                        <!-- Табл.4.19. Контроль возможности присутствия  ОКСМ в зависимости от значения ТипПредСд -->
                                                                        <sch:assert test="usch:iif(@ТипПредСд=1, count(ПерПредСд/@ОКСМ)!=0, (count(ПерПредСд/@ОКСМ)!=0 or count(ПерПредСд/@ОКСМ)=0) )">
                                                                            <usch:error code="0400300003" path="ПерПредСд/@ОКСМ" value-of-select="@ТипПредСд">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/УвКонтрСд/СвПредмСд/ПерПредСд/ОКСМ при значении элемента ТипПредСд, равном "<usch:value-of select="@ТипПредСд"/>".</usch:error>
                                                                        </sch:assert>
                                                                        <!-- Табл.4.19. Контроль обязательности присутствия  КодУсловПост в зависимости от значения ТипПредСд -->
                                                                        <sch:assert test="usch:iif(@ТипПредСд=1,
																		count(ПерПредСд/@КодУсловПост)=0 or  count(ПерПредСд/@КодУсловПост)!=0, 
																		 count(ПерПредСд/@КодУсловПост)=0) ">
                                                                            <usch:error code="0400300003" path="ПерПредСд/@КодУсловПост" value-of-select="@ТипПредСд">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/УвКонтрСд/СвПредмСд/ПерПредСд/КодУсловПост при значении элемента ТипПредСд, равном "<usch:value-of select="@ТипПредСд"/>".</usch:error>
                                                                        </sch:assert>
                                                                    </sch:rule>
                                                                </sch:pattern>
                                                            </xs:appinfo>
                                                            <!---*********************************************************************************************************************************-->
                                                            <xs:documentation>Сведения о предмете сделки (группы однородных сделок)</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="ПерПредСд" maxOccurs="unbounded">
                                                                    <xs:annotation>
                                                                        <!---*************************************************************************************************************************-->
                                                                        <xs:appinfo>
                                                                            <sch:pattern name="Присутствие КодРегион из МестОтпрТов  и  МестСовСд  в зависимости от  значения  ОКСМ">
                                                                                <sch:rule context="ПерПредСд">
                                                                                    <!-- Табл.4.24. Контроль обязательности присутствия  КодРегион из МестОтпрТов в зависимости от значения ОКСМ -->
                                                                                    <sch:assert test="usch:iif(МестОтпрТов/@ОКСМ=643, count(МестОтпрТов/@КодРегион)!=0 ,  (count(МестОтпрТов/@КодРегион)!=0 or count(МестОтпрТов/@КодРегион)=0))">
                                                                                        <usch:error code="0400300003" path="МестОтпрТов/@КодРегион" value-of-select="МестОтпрТов/@ОКСМ">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/УвКонтрСд/СвПредмСд/ПерПредСд/МестОтпрТов/@КодРегион при значении элемента ОКСМ, равном "<usch:value-of select="МестОтпрТов/@ОКСМ"/>".</usch:error>
                                                                                    </sch:assert>
                                                                                    <!-- Табл.4.24. Контроль обязательности присутствия  КодРегион из МестСовСд в зависимости от значения ОКСМ -->
                                                                                    <sch:assert test="usch:iif(МестСовСд/@ОКСМ=643, count(МестСовСд/@КодРегион)!=0 ,  (count(МестСовСд/@КодРегион)!=0 or count(МестСовСд/@КодРегион)=0))">
                                                                                        <usch:error code="0400300003" path="МестСовСд/@КодРегион" value-of-select="МестСовСд/@ОКСМ">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/УвКонтрСд/СвПредмСд/ПерПредСд/МестСовСд/@КодРегион при значении элемента ОКСМ, равном "<usch:value-of select="МестСовСд/@ОКСМ"/>".</usch:error>
                                                                                    </sch:assert>
                                                                                </sch:rule>
                                                                            </sch:pattern>
                                                                        </xs:appinfo>
                                                                        <!---*************************************************************************************************************************-->
                                                                        <xs:documentation>Перечень предметов сделки (группы однородных сделок)</xs:documentation>
                                                                    </xs:annotation>
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element name="МестОтпрТов" type="МестоТип" minOccurs="0">
                                                                                <xs:annotation>
                                                                                    <xs:documentation>Место отправки (погрузки) товара в соответствии с товаросопроводительными документами (заполняется только для товаров)</xs:documentation>
                                                                                </xs:annotation>
                                                                            </xs:element>
                                                                            <xs:element name="МестСовСд" type="МестоТип" minOccurs="0">
                                                                                <xs:annotation>
                                                                                    <xs:documentation>Место совершения сделки (адрес места доставки (разгрузки) товара; оказания услуги, работы; совершения сделки с иными объектами гражданских прав)</xs:documentation>
                                                                                </xs:annotation>
                                                                            </xs:element>
                                                                        </xs:sequence>
                                                                        <xs:attribute name="НаимПредСд" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Наименование предмета сделки</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:string">
                                                                                    <xs:minLength value="1"/>
                                                                                    <xs:maxLength value="512"/>
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="ТНВЭД" use="optional">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Код предмета сделки (по ТНВЭД)</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:string">
                                                                                    <xs:length value="10"/>
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="ОКП" use="optional">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Код предмета сделки (по ОКП)</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:string">
                                                                                    <xs:length value="6"/>
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="ОКВЭД" type="ОКВЭДТип" use="optional">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Код предмета сделки (по ОКВЭД)</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="НомУчСд" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Номер другого участника сделки</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:integer">
                                                                                    <xs:totalDigits value="4"/>
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="НомДог" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Номер договора</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:string">
                                                                                    <xs:minLength value="1"/>
                                                                                    <xs:maxLength value="128"/>
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="ДатаДог" type="ДатаТип" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Дата договора</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="ОКСМ" type="ОКСМТип" use="optional">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Код страны происхождения предмета сделки по классификатору ОКСМ (числовой)</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="КодУсловПост" use="optional">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Код условия поставки
                                                                                    (заполняется только для товаров)</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:string">
                                                                                    <xs:length value="3"/>
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="ОКЕИ" type="ОКЕИТип" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Код единицы измерения по ОКЕИ</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="Количество" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Количество</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:integer">
                                                                                    <xs:totalDigits value="15"/>
                                                                                    <xs:minInclusive value="1"/>
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="ЦенаЕдин" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Цена (тариф) за единицу измерения без учета НДС, акцизов и пошлины</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:integer">
                                                                                    <xs:totalDigits value="15"/>
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="СтоимИтог" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Итого стоимость без учета НДС, акцизов и пошлины</xs:documentation>
                                                                            </xs:annotation>
                                                                            <xs:simpleType>
                                                                                <xs:restriction base="xs:integer">
                                                                                    <xs:totalDigits value="15"/>
                                                                                </xs:restriction>
                                                                            </xs:simpleType>
                                                                        </xs:attribute>
                                                                        <xs:attribute name="ДатаСовСд" type="ДатаТип" use="required">
                                                                            <xs:annotation>
                                                                                <xs:documentation>Дата совершения сделки</xs:documentation>
                                                                            </xs:annotation>
                                                                        </xs:attribute>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                            <xs:attribute name="ТипПредСд" use="required">
                                                                <xs:annotation>
                                                                    <xs:documentation>Тип предмета сделки</xs:documentation>
                                                                </xs:annotation>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:length value="1"/>
                                                                        <xs:enumeration value="1"/>
                                                                        <xs:enumeration value="2"/>
                                                                        <xs:enumeration value="3"/>
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="СвОргУчаст" minOccurs="0" maxOccurs="unbounded">
                                                        <xs:annotation>
                                                            <!---*********************************************************************************************************************************-->
                                                            <xs:appinfo>
                                                                <sch:pattern name="Присутствие ИННЮЛ, КПП, АдрИнТекст, РегНомИн, КодНПРег в зависимости от  значения  ПрОрг">
                                                                    <sch:rule context="СвОргУчаст">
                                                                        <!-- Табл.4.20. Контроль обязательности присутствия  ИННЮЛ в зависимости от значения ПрОрг -->
                                                                        <sch:assert test="usch:iif(@ПрОрг=1, count(@ИННЮЛ)!=0 ,  (count(@ИННЮЛ)!=0 or count(@ИННЮЛ)=0))">
                                                                            <usch:error code="0400300003" path="@ИННЮЛ" value-of-select="@ПрОрг">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/УвКонтрСд/СвОргУчаст/ИННЮЛ при значении элемента ПрОрг, равном "<usch:value-of select="@ПрОрг"/>".</usch:error>
                                                                        </sch:assert>
                                                                        <!-- Табл.4.20. Контроль обязательности присутствия  КПП в зависимости от значения ПрОрг -->
                                                                        <sch:assert test="usch:iif(@ПрОрг=1, count(@КПП)!=0 ,  (count(@КПП)!=0 or count(@КПП)=0))">
                                                                            <usch:error code="0400300003" path="@КПП" value-of-select="@ПрОрг">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/УвКонтрСд/СвОргУчаст/КПП при значении элемента ПрОрг, равном "<usch:value-of select="@ПрОрг"/>".</usch:error>
                                                                        </sch:assert>
                                                                        <!-- Табл.4.20. Контроль возможности присутствия  АдрИнТекст в зависимости от значения ПрОрг -->
                                                                        <sch:assert test="usch:iif(@ПрОрг=2, (count(@АдрИнТекст)!=0 or count(@АдрИнТекст)=0),  count(@АдрИнТекст)=0)">
                                                                            <usch:error code="0400300003" path="@АдрИнТекст" value-of-select="@ПрОрг">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/УвКонтрСд/СвОргУчаст/АдрИнТекст при значении элемента ПрОрг, равном "<usch:value-of select="@ПрОрг"/>".</usch:error>
                                                                        </sch:assert>
                                                                        <!-- Табл.4.20. Контроль возможности присутствия  РегНомИн в зависимости от значения ПрОрг -->
                                                                        <sch:assert test="usch:iif(@ПрОрг=2, (count(@РегНомИн)!=0 or count(@РегНомИн)=0),  count(@РегНомИн)=0)">
                                                                            <usch:error code="0400300003" path="@РегНомИн" value-of-select="@ПрОрг">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/УвКонтрСд/СвОргУчаст/РегНомИн при значении элемента ПрОрг, равном "<usch:value-of select="@ПрОрг"/>".</usch:error>
                                                                        </sch:assert>
                                                                        <!-- Табл.4.20. Контроль обязательности присутствия  РегНомИн в зависимости от значения ПрОрг и наличия КодНПРег -->
                                                                        <sch:assert test="usch:iif( (@ПрОрг=2 and count(@КодНПРег)=0) , count(@РегНомИн)!=0 ,                 (count(@РегНомИн)!=0 or count(@РегНомИн)=0))">
                                                                            <usch:error code="0400300003" path="@РегНомИн" value-of-select="@ПрОрг">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/УвКонтрСд/СвОргУчаст/РегНомИн при значении элемента ПрОрг, равном "<usch:value-of select="@ПрОрг"/>" и отсутствии элемента КодНПРег.</usch:error>
                                                                        </sch:assert>
                                                                        <!-- Табл.4.20. Контроль возможности присутствия  КодНПРег в зависимости от значения ПрОрг -->
                                                                        <sch:assert test="usch:iif(@ПрОрг=2, (count(@КодНПРег)!=0 or count(@КодНПРег)=0),  count(@КодНПРег)=0)">
                                                                            <usch:error code="0400300003" path="@КодНПРег" value-of-select="@ПрОрг">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/УвКонтрСд/СвОргУчаст/КодНПРег при значении элемента ПрОрг, равном "<usch:value-of select="@ПрОрг"/>".</usch:error>
                                                                        </sch:assert>
                                                                        <!-- Табл.4.20. Контроль обязательности присутствия  КодНПРег в зависимости от значения ПрОрг и наличия РегНомИн -->
                                                                        <sch:assert test="usch:iif( (@ПрОрг=2 and count(@РегНомИн)=0) , count(@КодНПРег)!=0 ,                 (count(@КодНПРег)!=0 or count(@КодНПРег)=0))">
                                                                            <usch:error code="0400300003" path="@КодНПРег" value-of-select="@ПрОрг">Не выполнено условие присутствия (отсутствия) элемента Файл/Документ/УвКонтрСд/СвОргУчаст/КодНПРег при значении элемента ПрОрг, равном "<usch:value-of select="@ПрОрг"/>" и отсутствии элемента РегНомИн.</usch:error>
                                                                        </sch:assert>
                                                                    </sch:rule>
                                                                </sch:pattern>
                                                            </xs:appinfo>
                                                            <!---*********************************************************************************************************************************-->
                                                            <xs:documentation>Сведения об организации - участнике контролируемой сделки (группы однородных сделок)</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:complexType>
                                                            <xs:attribute name="НомПорСд" use="required">
                                                                <xs:annotation>
                                                                    <xs:documentation>Порядковый номер участника  сделки </xs:documentation>
                                                                </xs:annotation>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:integer">
                                                                        <xs:totalDigits value="4"/>
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:attribute>
                                                            <xs:attribute name="ПрОрг" use="required">
                                                                <xs:annotation>
                                                                    <xs:documentation>Признак организации</xs:documentation>
                                                                </xs:annotation>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:length value="1"/>
                                                                        <xs:enumeration value="1"/>
                                                                        <xs:enumeration value="2"/>
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:attribute>
                                                            <xs:attribute name="ОКСМ" type="ОКСМТип" use="required">
                                                                <xs:annotation>
                                                                    <xs:documentation>Код страны по классификатору ОКСМ (числовой)</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="НаимОрг" use="required">
                                                                <xs:annotation>
                                                                    <xs:documentation>Наименование организации</xs:documentation>
                                                                </xs:annotation>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:minLength value="1"/>
                                                                        <xs:maxLength value="1000"/>
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:attribute>
                                                            <xs:attribute name="ИННЮЛ" type="ИННЮЛТип" use="optional">
                                                                <xs:annotation>
                                                                    <xs:documentation>ИНН организации</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="КПП" type="КППТип" use="optional">
                                                                <xs:annotation>
                                                                    <xs:documentation>КПП</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="РегНомИн" use="optional">
                                                                <xs:annotation>
                                                                    <xs:documentation>Регистрационный номер организации в стране ее регистрации (инкорпорации)</xs:documentation>
                                                                </xs:annotation>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:minLength value="1"/>
                                                                        <xs:maxLength value="50"/>
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:attribute>
                                                            <xs:attribute name="КодНПРег" use="optional">
                                                                <xs:annotation>
                                                                    <xs:documentation>Код налогоплательщика в стране регистрации (инкорпорации) или его аналог</xs:documentation>
                                                                </xs:annotation>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:minLength value="1"/>
                                                                        <xs:maxLength value="50"/>
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:attribute>
                                                            <xs:attribute name="АдрИнТекст" use="optional">
                                                                <xs:annotation>
                                                                    <xs:documentation>Адрес (для иностранных организаций)</xs:documentation>
                                                                </xs:annotation>
                                                                <xs:simpleType>
                                                                    <xs:restriction base="xs:string">
                                                                        <xs:minLength value="1"/>
                                                                        <xs:maxLength value="255"/>
                                                                    </xs:restriction>
                                                                </xs:simpleType>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="СвФЛУчаст" minOccurs="0" maxOccurs="unbounded">
                                                        <xs:annotation>
                                                            <xs:documentation>Сведения о физическом лице - участнике контролируемой сделки (группы однородных сделок)</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="ФИО" type="ФИОТип">
                                                                    <xs:annotation>
                                                                        <xs:documentation>Фамилия, имя, отчество физического лица</xs:documentation>
                                                                    </xs:annotation>
                                                                </xs:element>
                                                                <xs:choice>
                                                                    <xs:element name="ИННФЛ" type="ИННФЛТип">
                                                                        <xs:annotation>
                                                                            <xs:documentation>ИНН физического лица</xs:documentation>
                                                                        </xs:annotation>
                                                                    </xs:element>
                                                                    <xs:element name="СвФЛ">
                                                                        <xs:annotation>
                                                                            <xs:documentation>Сведения о физическом  лице</xs:documentation>
                                                                        </xs:annotation>
                                                                        <xs:complexType>
                                                                            <xs:sequence>
                                                                                <xs:element name="УдЛичнФЛ" type="УдЛичнФЛТип">
                                                                                    <xs:annotation>
