<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>false</fixedRows>
    <name>Предоставление гарантий</name>
    <fullName>Отчет в отношении доходов ОАО «Сбербанк России» по Контролируемым сделкам, связанным с предоставлением
        гарантий
    </fullName>
    <code>ПРИЛОЖЕНИЕ 2 |к Распоряжению | от "__" апреля 2013 №___</code>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="autoNumerationColumn">
        <alias>rowNumber</alias>
        <checking>false</checking>
        <columnType>AUTO</columnType>
        <name>№ п/п</name>
        <order>1</order>
        <width>4</width>
        <numerationType>SERIAL</numerationType>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>itog</alias>
        <checking>false</checking>
        <name>itog</name>
        <order>2</order>
        <width>0</width>
        <maxLength>100</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>fullName</alias>
        <checking>false</checking>
        <name>Полное наименование с указанием ОПФ</name>
        <order>3</order>
        <width>10</width>
        <hierarchical>false</hierarchical>
        <nameAttributeId>32</nameAttributeId>
        <refBookAttributeId>32</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="referenceColumn">
        <alias>inn</alias>
        <checking>false</checking>
        <name>ИНН/КИО</name>
        <order>4</order>
        <width>7</width>
        <parentAlias>fullName</parentAlias>
        <refBookAttributeId>37</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="referenceColumn">
        <alias>countryName</alias>
        <checking>false</checking>
        <name>Страна регистрации</name>
        <order>5</order>
        <width>8</width>
        <parentAlias>fullName</parentAlias>
        <refBookAttributeId>33</refBookAttributeId>
        <refBookAttributeId2>50</refBookAttributeId2>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>docNumber</alias>
        <checking>false</checking>
        <name>Номер договора</name>
        <order>6</order>
        <width>5</width>
        <maxLength>128</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="dateColumn">
        <alias>docDate</alias>
        <checking>false</checking>
        <name>Дата договора</name>
        <order>7</order>
        <width>7</width>
        <formatId>0</formatId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>dealNumber</alias>
        <checking>false</checking>
        <name>Номер сделки</name>
        <order>8</order>
        <width>5</width>
        <maxLength>500</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="dateColumn">
        <alias>dealDate</alias>
        <checking>false</checking>
        <name>Дата сделки</name>
        <order>9</order>
        <width>7</width>
        <formatId>0</formatId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>sum</alias>
        <checking>false</checking>
        <name>Сумма доходов Банка по данным бухгалтерского учета, руб.</name>
        <order>10</order>
        <width>8</width>
        <maxLength>15</maxLength>
        <precision>0</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>price</alias>
        <checking>false</checking>
        <name>Цена (тариф) за единицу измерения без учета НДС, акцизов и пошлины, руб.</name>
        <order>11</order>
        <width>8</width>
        <maxLength>15</maxLength>
        <precision>0</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>total</alias>
        <checking>false</checking>
        <name>Итого стоимость без учета НДС, акцизов и пошлин, руб.</name>
        <order>12</order>
        <width>8</width>
        <maxLength>15</maxLength>
        <precision>0</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>fix</alias>
        <checking>false</checking>
        <name>fix</name>
        <order>13</order>
        <width>0</width>
        <maxLength>1000</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="dateColumn">
        <alias>dealDoneDate</alias>
        <checking>false</checking>
        <name>Дата совершения сделки</name>
        <order>14</order>
        <width>7</width>
        <formatId>0</formatId>
    </columns>
    <styles>
        <alias>Корректировка-изменено</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-без изменений</alias>
        <backColor>GREY</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-добавлено</alias>
        <backColor>PALE_GREEN</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-удалено</alias>
        <backColor>LIGHT_CORAL</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Редактируемая</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Автозаполняемая</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>DARK_GREEN</fontColor>
        <italic>false</italic>
    </styles>
</formTemplateContent>