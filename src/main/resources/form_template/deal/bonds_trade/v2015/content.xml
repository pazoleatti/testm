<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>false</fixedRows>
    <name>Реализация и приобретение ценных бумаг</name>
    <fullName>Отчет в отношении доходов и расходов ПАО Сбербанк по Контролируемым сделкам, связанным с реализацией и приобретением ценных бумаг</fullName>
    <code>ПРИЛОЖЕНИЕ 2|к Распоряжению|от "__" марта 2013 № ___</code>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="autoNumerationColumn">
        <alias>rowNum</alias>
        <checking>false</checking>
        <columnType>AUTO</columnType>
        <name>№ п/п</name>
        <order>1</order>
        <width>4</width>
        <numerationType>SERIAL</numerationType>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="dateColumn">
        <alias>transactionDeliveryDate</alias>
        <checking>false</checking>
        <name>Дата сделки (поставки)</name>
        <order>2</order>
        <width>7</width>
        <formatId>0</formatId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>contraName</alias>
        <checking>false</checking>
        <name>Наименование контрагента и ОПФ</name>
        <order>3</order>
        <width>10</width>
        <hierarchical>false</hierarchical>
        <nameAttributeId>32</nameAttributeId>
        <refBookAttributeId>32</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>transactionMode</alias>
        <checking>false</checking>
        <name>Режим переговорных сделок</name>
        <order>4</order>
        <width>6</width>
        <hierarchical>false</hierarchical>
        <nameAttributeId>63</nameAttributeId>
        <refBookAttributeId>63</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="referenceColumn">
        <alias>innKio</alias>
        <checking>false</checking>
        <name>ИНН/КИО контрагента</name>
        <order>5</order>
        <width>7</width>
        <parentAlias>contraName</parentAlias>
        <refBookAttributeId>37</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="referenceColumn">
        <alias>contraCountry</alias>
        <checking>false</checking>
        <name>Страна местонахождения контрагента</name>
        <order>6</order>
        <width>8</width>
        <parentAlias>contraName</parentAlias>
        <refBookAttributeId>33</refBookAttributeId>
        <refBookAttributeId2>53</refBookAttributeId2>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="referenceColumn">
        <alias>contraCountryCode</alias>
        <checking>false</checking>
        <name>Код страны местонахождения контрагента</name>
        <order>7</order>
        <width>8</width>
        <parentAlias>contraName</parentAlias>
        <refBookAttributeId>33</refBookAttributeId>
        <refBookAttributeId2>50</refBookAttributeId2>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>transactionSumCurrency</alias>
        <checking>false</checking>
        <name>Сумма сделки (с учетом НКД), в валюте расчетов.</name>
        <order>8</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>currency</alias>
        <checking>false</checking>
        <name>Валюта расчетов по сделке</name>
        <order>9</order>
        <width>5</width>
        <hierarchical>false</hierarchical>
        <nameAttributeId>66</nameAttributeId>
        <refBookAttributeId>64</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>courseCB</alias>
        <checking>false</checking>
        <name>Курс ЦБ РФ</name>
        <order>10</order>
        <width>5</width>
        <maxLength>19</maxLength>
        <precision>4</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>transactionSumRub</alias>
        <checking>false</checking>
        <name>Сумма сделки (с учетом НКД), руб.</name>
        <order>11</order>
        <width>8</width>
        <maxLength>15</maxLength>
        <precision>0</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>contractNum</alias>
        <checking>false</checking>
        <name>Номер договора</name>
        <order>12</order>
        <width>5</width>
        <maxLength>128</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="dateColumn">
        <alias>contractDate</alias>
        <checking>false</checking>
        <name>Дата договора</name>
        <order>13</order>
        <width>7</width>
        <formatId>0</formatId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="dateColumn">
        <alias>transactionDate</alias>
        <checking>false</checking>
        <name>Дата заключения сделки</name>
        <order>14</order>
        <width>7</width>
        <formatId>0</formatId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>bondRegCode</alias>
        <checking>false</checking>
        <name>Регистрационный код ценной бумаги</name>
        <order>15</order>
        <width>8</width>
        <maxLength>500</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>bondCount</alias>
        <checking>false</checking>
        <name>Количество бумаг по сделке, шт.</name>
        <order>16</order>
        <width>5</width>
        <maxLength>15</maxLength>
        <precision>0</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>priceOne</alias>
        <checking>false</checking>
        <name>Цена за 1 шт., руб.</name>
        <order>17</order>
        <width>8</width>
        <maxLength>15</maxLength>
        <precision>0</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>transactionType</alias>
        <checking>false</checking>
        <name>Тип сделки</name>
        <order>18</order>
        <width>5</width>
        <hierarchical>false</hierarchical>
        <nameAttributeId>69</nameAttributeId>
        <refBookAttributeId>69</refBookAttributeId>
    </columns>
    <styles>
        <alias>Корректировка-изменено</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-без изменений</alias>
        <backColor>GREY</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-добавлено</alias>
        <backColor>PALE_GREEN</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-удалено</alias>
        <backColor>LIGHT_CORAL</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Редактируемая</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Автозаполняемая</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>DARK_GREEN</fontColor>
        <italic>false</italic>
    </styles>
</formTemplateContent>