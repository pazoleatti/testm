<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>true</fixedRows>
    <name>Сводная форма начисленных доходов (доходы сложные)</name>
    <fullName>Сводная форма начисленных доходов</fullName>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>incomeTypeId</alias>
        <checking>false</checking>
        <name>КНУ</name>
        <order>1</order>
        <width>3</width>
        <maxLength>5</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>incomeGroup</alias>
        <checking>false</checking>
        <name>Группа дохода</name>
        <order>2</order>
        <width>15</width>
        <maxLength>500</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>incomeTypeByOperation</alias>
        <checking>false</checking>
        <name>Вид дохода по операциям</name>
        <order>3</order>
        <width>28</width>
        <maxLength>500</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>incomeBuhSumAccountNumber</alias>
        <checking>false</checking>
        <name>Доход по данным бухгалтерского учёта. Балансовый счёт по учёту дохода</name>
        <order>4</order>
        <width>7</width>
        <maxLength>15</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>incomeBuhSumRnuSource</alias>
        <checking>false</checking>
        <name>Доход по данным бухгалтерского учёта. Источник информации в РНУ</name>
        <order>5</order>
        <width>7</width>
        <maxLength>255</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>incomeBuhSumAccepted</alias>
        <checking>false</checking>
        <name>Доход по данным бухгалтерского учёта. Сумма</name>
        <order>6</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>incomeBuhSumPrevTaxPeriod</alias>
        <checking>false</checking>
        <name>Доход по данным бухгалтерского учёта. В т.ч. учтено в предыдущих налоговых периодах</name>
        <order>7</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>incomeTaxSumRnuSource</alias>
        <checking>false</checking>
        <name>Доход по данным налогового учёта. Источник информации в РНУ</name>
        <order>8</order>
        <width>8</width>
        <maxLength>255</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>incomeTaxSumS</alias>
        <checking>false</checking>
        <name>Доход по данным налогового учёта. Сумма</name>
        <order>9</order>
        <width>12</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>rnuNo</alias>
        <checking>false</checking>
        <name>Доход по данным налогового учёта. Форма РНУ</name>
        <order>10</order>
        <width>5</width>
        <maxLength>100</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>logicalCheck</alias>
        <checking>true</checking>
        <name>Логическая проверка</name>
        <order>11</order>
        <width>7</width>
        <maxLength>255</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>accountingRecords</alias>
        <checking>true</checking>
        <name>Счёт бухгалтерского учёта</name>
        <order>12</order>
        <width>7</width>
        <maxLength>10</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>opuSumByEnclosure2</alias>
        <checking>true</checking>
        <name>Сумма по бухгалтерскому учёту. В Приложении №5</name>
        <order>13</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>opuSumByTableD</alias>
        <checking>true</checking>
        <name>Сумма по бухгалтерскому учёту. В Таблице "Д"</name>
        <order>14</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>opuSumTotal</alias>
        <checking>true</checking>
        <name>Сумма по бухгалтерскому учёту. В бухгалтерской отчётности</name>
        <order>15</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>explanation</alias>
        <checking>true</checking>
        <name>Сумма по бухгалтерскому учёту. Пояснение отклонений, таблица 1 (графа 4)</name>
        <order>16</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>difference</alias>
        <checking>true</checking>
        <name>Расхождение</name>
        <order>17</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <styles>
        <alias>Корректировка-изменено</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-без изменений</alias>
        <backColor>GREY</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-добавлено</alias>
        <backColor>PALE_GREEN</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-удалено</alias>
        <backColor>LIGHT_CORAL</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>17 Числовое значение в КС</alias>
        <backColor>RED</backColor>
        <bold>true</bold>
        <fontColor>WHITE</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Числовое значение</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
