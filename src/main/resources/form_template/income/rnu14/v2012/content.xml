<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>true</fixedRows>
    <name>(РНУ-14) Регистр налогового учёта нормируемых расходов</name>
    <fullName>(РНУ-14) Регистр налогового учёта нормируемых расходов</fullName>
    <code>РНУ-14</code>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>knu</alias>
        <checking>false</checking>
        <name>КНУ</name>
        <order>1</order>
        <width>4</width>
        <maxLength>1000</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>mode</alias>
        <checking>false</checking>
        <name>Вид нормируемых расходов</name>
        <order>2</order>
        <width>40</width>
        <maxLength>255</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>sum</alias>
        <checking>false</checking>
        <name>Сумма расходов по данным налогового учёта</name>
        <order>3</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>normBase</alias>
        <checking>false</checking>
        <name>База для расчёта нормы расходов</name>
        <order>4</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>normCoef</alias>
        <checking>false</checking>
        <name>Коэффициент</name>
        <order>5</order>
        <width>8</width>
        <maxLength>10</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>limitSum</alias>
        <checking>false</checking>
        <name>Предельная сумма расходов, учитываемая для целей налогообложения.</name>
        <order>6</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>inApprovedNprms</alias>
        <checking>false</checking>
        <name>В пределах утверждённых норм</name>
        <order>7</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>overApprovedNprms</alias>
        <checking>false</checking>
        <name>Сверх утверждённых норм.</name>
        <order>8</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <styles>
        <alias>Корректировка-изменено</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-без изменений</alias>
        <backColor>GREY</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-добавлено</alias>
        <backColor>PALE_GREEN</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-удалено</alias>
        <backColor>LIGHT_CORAL</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Редактируемая</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Вычисляемая</alias>
        <backColor>BLACK</backColor>
        <bold>false</bold>
        <fontColor>WHITE</fontColor>
        <italic>false</italic>
    </styles>
</formTemplateContent>