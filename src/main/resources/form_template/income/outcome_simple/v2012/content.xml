<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>true</fixedRows>
    <name>Расходы, учитываемые в простых РНУ (расходы простые)</name>
    <fullName>Расходы, учитываемые в простых РНУ Сбербанка (расходы простые)</fullName>
    <header>787/Р/1</header>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>consumptionTypeId</alias>
        <checking>false</checking>
        <name>КНУ</name>
        <order>1</order>
        <width>3</width>
        <maxLength>5</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>consumptionGroup</alias>
        <checking>false</checking>
        <name>Группа расхода</name>
        <order>2</order>
        <width>15</width>
        <maxLength>500</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>consumptionTypeByOperation</alias>
        <checking>false</checking>
        <name>Вид расхода по операциям</name>
        <order>3</order>
        <width>25</width>
        <maxLength>1000</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>consumptionAccountNumber</alias>
        <checking>false</checking>
        <name>Балансовый счёт по учёту расхода</name>
        <order>4</order>
        <width>7</width>
        <maxLength>15</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>rnu7Field10Sum</alias>
        <checking>false</checking>
        <name>РНУ-7 (графа 10) сумма</name>
        <order>5</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>rnu7Field12Accepted</alias>
        <checking>false</checking>
        <name>сумма</name>
        <order>6</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>rnu7Field12PrevTaxPeriod</alias>
        <checking>false</checking>
        <name>в т.ч. учтено в предыдущих налоговых периодах по графе 10</name>
        <order>7</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>rnu5Field5Accepted</alias>
        <checking>false</checking>
        <name>сумма</name>
        <order>8</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>logicalCheck</alias>
        <checking>true</checking>
        <name>Логическая проверка</name>
        <order>9</order>
        <width>7</width>
        <maxLength>255</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>accountingRecords</alias>
        <checking>false</checking>
        <name>Счёт бухгалтерского учёта</name>
        <order>10</order>
        <width>7</width>
        <maxLength>10</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>opuSumByEnclosure2</alias>
        <checking>true</checking>
        <name>в Приложении №6</name>
        <order>11</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>opuSumByTableP</alias>
        <checking>true</checking>
        <name>в Таблице &quot;Р&quot;</name>
        <order>12</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>opuSumTotal</alias>
        <checking>true</checking>
        <name>в бухгалтерской отчётности</name>
        <order>13</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>difference</alias>
        <checking>true</checking>
        <name>Расхождение</name>
        <order>14</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <styles>
        <alias>Голубой</alias>
        <backColor>BLUE</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Заголовок подраздела (светло-желтый)</alias>
        <backColor>LIGHT_YELLOW</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Заголовок раздела (серый)</alias>
        <backColor>GREY</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Итого (светло-коричневый)</alias>
        <backColor>LIGHT_BROWN</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Логическая проверка</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Расхождение</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>DARK_BLUE</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Редактирование (светло-голубой)</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Спец. значение в контрольных графах</alias>
        <backColor>RED</backColor>
        <bold>true</bold>
        <fontColor>WHITE</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Строковое значение КП</alias>
        <backColor>GREY</backColor>
        <bold>true</bold>
        <fontColor>DARK_BLUE</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Строковое значение в контрольных суммах</alias>
        <backColor>LIGHT_BROWN</backColor>
        <bold>true</bold>
        <fontColor>DARK_BLUE</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Числовое значение КП</alias>
        <backColor>WHITE</backColor>
        <bold>false</bold>
        <fontColor>DARK_BLUE</fontColor>
        <italic>false</italic>
    </styles>
</formTemplateContent>