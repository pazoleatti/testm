<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>true</fixedRows>
    <monthly>false</monthly>
    <comparative>false</comparative>
    <accruing>false</accruing>
    <updating>false</updating>
    <name>Сводная форма начисленных расходов (расходы сложные)</name>
    <fullName>Сводная форма начисленных расходов</fullName>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>consumptionTypeId</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>КНУ</name>
        <order>1</order>
        <width>3</width>
        <maxLength>50</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>consumptionGroup</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>Группа расхода</name>
        <order>2</order>
        <width>15</width>
        <maxLength>2000</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>consumptionTypeByOperation</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>Вид расхода по операции</name>
        <order>3</order>
        <width>25</width>
        <maxLength>2000</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>consumptionBuhSumAccountNumber</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>номер счёта учёта</name>
        <order>4</order>
        <width>7</width>
        <maxLength>20</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>consumptionBuhSumRnuSource</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>источник информации в РНУ</name>
        <order>5</order>
        <width>7</width>
        <maxLength>255</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>consumptionBuhSumAccepted</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>сумма</name>
        <order>6</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>consumptionBuhSumPrevTaxPeriod</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>в т.ч. учтено в предыдущих налоговых периодах</name>
        <order>7</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>consumptionTaxSumRnuSource</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>источник информации в РНУ</name>
        <order>8</order>
        <width>7</width>
        <maxLength>255</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>consumptionTaxSumS</alias>
        <checking>false</checking>
        <columnType>NUMBER</columnType>
        <name>сумма</name>
        <order>9</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>rnuNo</alias>
        <checking>false</checking>
        <columnType>STRING</columnType>
        <name>Форма РНУ</name>
        <order>10</order>
        <width>5</width>
        <maxLength>50</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>logicalCheck</alias>
        <checking>true</checking>
        <columnType>STRING</columnType>
        <name>Логическая проверка</name>
        <order>11</order>
        <width>7</width>
        <maxLength>255</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>accountingRecords</alias>
        <checking>true</checking>
        <columnType>STRING</columnType>
        <name>Счёт бухгалтерского учёта</name>
        <order>12</order>
        <width>8</width>
        <maxLength>10</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>opuSumByEnclosure3</alias>
        <checking>true</checking>
        <columnType>NUMBER</columnType>
        <name>в Приложении №6</name>
        <order>13</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>opuSumByTableP</alias>
        <checking>true</checking>
        <columnType>NUMBER</columnType>
        <name>в Таблице "Р"</name>
        <order>14</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>opuSumTotal</alias>
        <checking>true</checking>
        <columnType>NUMBER</columnType>
        <name>в бухгалтерской отчётности</name>
        <order>15</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>table2</alias>
        <checking>true</checking>
        <columnType>NUMBER</columnType>
        <name>пояснения Таблица 2 (графа 4)</name>
        <order>16</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>difference</alias>
        <checking>true</checking>
        <columnType>NUMBER</columnType>
        <name>Расхождение</name>
        <order>17</order>
        <width>8</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <styles>
        <alias>Корректировка-изменено</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-без изменений</alias>
        <backColor>GREY</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-добавлено</alias>
        <backColor>PALE_GREEN</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Корректировка-удалено</alias>
        <backColor>LIGHT_CORAL</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Редактирование (светло-голубой)</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Итого (светло-коричневый)</alias>
        <backColor>LIGHT_BROWN</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Заголовок раздела (серый)</alias>
        <backColor>GREY</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Заголовок подраздела (светло-желтый)</alias>
        <backColor>LIGHT_YELLOW</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Голубой</alias>
        <backColor>BLUE</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Синий</alias>
        <backColor>DARK_BLUE</backColor>
        <bold>false</bold>
        <fontColor>WHITE</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Значение в столбце "Логическая проверка"</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>RED</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Спец. в столбце "Логическая проверка"</alias>
        <backColor>RED</backColor>
        <bold>true</bold>
        <fontColor>WHITE</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Строковое значение</alias>
        <backColor>GREY</backColor>
        <bold>true</bold>
        <fontColor>DARK_BLUE</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Числовое значение</alias>
        <backColor>WHITE</backColor>
        <bold>false</bold>
        <fontColor>DARK_BLUE</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Значение в столбце "Расхождение"</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>DARK_BLUE</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Строковое значение в контрольных суммах</alias>
        <backColor>LIGHT_BROWN</backColor>
        <bold>true</bold>
        <fontColor>DARK_BLUE</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Числовое значение в контрольных суммах</alias>
        <backColor>RED</backColor>
        <bold>true</bold>
        <fontColor>WHITE</fontColor>
        <italic>false</italic>
    </styles>
</formTemplateContent>