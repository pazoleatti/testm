        <alias>tadeNumber</alias>
        <checking>false</checking>
        <name>Номер сделки (первая часть)</name>
        <order>1</order>
        <width>10</width>
        <maxLength>30</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>securityName</alias>
        <checking>false</checking>
        <name>Наименование ценной бумаги</name>
        <order>2</order>
        <width>10</width>
        <maxLength>255</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>currencyCode</alias>
        <checking>false</checking>
        <name>Код валюты</name>
        <order>3</order>
        <width>5</width>
