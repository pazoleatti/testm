<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<formTemplateContent>
    <fixedRows>true</fixedRows>
    <name>Расчёт налога на имущество по кадастровой стоимости</name>
    <fullName>Расчёт налога на имущество по кадастровой стоимости</fullName>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="autoNumerationColumn">
        <alias>rowNum</alias>
        <checking>false</checking>
        <name>№ пп</name>
        <order>1</order>
        <width>5</width>
        <type>0</type>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>fix</alias>
        <checking>false</checking>
        <name>fix</name>
        <order>2</order>
        <width>0</width>
        <maxLength>100</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>subject</alias>
        <checking>false</checking>
        <name>Код субъекта</name>
        <order>3</order>
        <width>5</width>
        <hierarchical>false</hierarchical>
        <nameAttributeId>10</nameAttributeId>
        <refBookAttributeId>9</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>taxAuthority</alias>
        <checking>false</checking>
        <name>Код НО</name>
        <order>4</order>
        <width>5</width>
        <maxLength>4</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>kpp</alias>
        <checking>false</checking>
        <name>КПП</name>
        <order>5</order>
        <width>5</width>
        <maxLength>9</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>oktmo</alias>
        <checking>false</checking>
        <name>Код ОКТМО</name>
        <order>6</order>
        <width>8</width>
        <hierarchical>true</hierarchical>
        <nameAttributeId>841</nameAttributeId>
        <refBookAttributeId>840</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>address</alias>
        <checking>false</checking>
        <name>Адрес объекта</name>
        <order>7</order>
        <width>5</width>
        <maxLength>255</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>sign</alias>
        <checking>false</checking>
        <name>Признак: здание - 1, помещение - 2</name>
        <order>8</order>
        <width>6</width>
        <maxLength>1</maxLength>
        <precision>0</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="stringColumn">
        <alias>cadastreNum</alias>
        <checking>false</checking>
        <name>Кадастровый номер</name>
        <order>9</order>
        <width>7</width>
        <maxLength>100</maxLength>
        <prevLength>0</prevLength>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>cadastrePriceJanuary</alias>
        <checking>false</checking>
        <name>Кадастровая стоимость на 1 января</name>
        <order>10</order>
        <width>5</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>cadastrePriceTaxFree</alias>
        <checking>false</checking>
        <name>Кадастровая стоимость в т.ч. необлагаемая налогом</name>
        <order>11</order>
        <width>7</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>tenure</alias>
        <checking>false</checking>
        <name>Срок владения, мес.</name>
        <order>12</order>
        <width>5</width>
        <maxLength>15</maxLength>
        <precision>0</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="refBookColumn">
        <alias>taxBenefitCode</alias>
        <checking>false</checking>
        <name>Код налоговой льготы</name>
        <order>13</order>
        <width>5</width>
        <filter>CODE IS NULL OR CODE = '2012000' OR CODE = '2012400' OR CODE = '2012500'</filter>
        <hierarchical>false</hierarchical>
        <nameAttributeId>2022</nameAttributeId>
        <refBookAttributeId>2021</refBookAttributeId>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>taxBase</alias>
        <checking>false</checking>
        <name>Налоговая база</name>
        <order>14</order>
        <width>5</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>taxRate</alias>
        <checking>false</checking>
        <name>Налоговая ставка</name>
        <order>15</order>
        <width>5</width>
        <maxLength>3</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>sum</alias>
        <checking>false</checking>
        <name>Сумма налога (авансового платежа)</name>
        <order>16</order>
        <width>6</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>periodSum</alias>
        <checking>false</checking>
        <name>Сумма авансовых платежей, исчисленная за отчетные периоды</name>
        <order>17</order>
        <width>10</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <columns xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="numericColumn">
        <alias>reductionPaymentSum</alias>
        <checking>false</checking>
        <name>Сумма уменьшения платежа</name>
        <order>18</order>
        <width>6</width>
        <maxLength>17</maxLength>
        <precision>2</precision>
    </columns>
    <styles>
        <alias>Контрольные суммы</alias>
        <backColor>LIGHT_BROWN</backColor>
        <bold>true</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Редактируемая</alias>
        <backColor>LIGHT_BLUE</backColor>
        <bold>false</bold>
        <fontColor>BLACK</fontColor>
        <italic>false</italic>
    </styles>
    <styles>
        <alias>Автозаполняемая</alias>
        <backColor>WHITE</backColor>
        <bold>true</bold>
        <fontColor>DARK_GREEN</fontColor>
        <italic>false</italic>
    </styles>
</formTemplateContent>