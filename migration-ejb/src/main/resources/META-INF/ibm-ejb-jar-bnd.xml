<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar-bnd xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://websphere.ibm.com/xml/ns/javaee"
    xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-ejb-jar-bnd_1_0.xsd" version="1.0">

    <session name="MessageServiceBean">
        <resource-ref binding-name="jms/transportConnectionFactory" name="jms/transportConnectionFactory${releasePostfix}"/>
        <resource-ref binding-name="jms/rateQueueConnectionFactory" name="jms/rateQueueConnectionFactory${releasePostfix}"/>
        <message-destination-ref binding-name="jms/transportQueue" name="jms/transportQueue${releasePostfix}"/>
        <message-destination-ref binding-name="jms/rateQueue" name="jms/rateQueue${releasePostfix}"/>
    </session>

</ejb-jar-bnd>