<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:jee="http://www.springframework.org/schema/jee"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-3.1.xsd">

    <jee:jndi-lookup id="migrationConnectionFactory" jndi-name="java:comp/env/jms/migrationConnectionFactory"/>
    <jee:jndi-lookup id="migrationQueue" jndi-name="java:comp/env/jms/migrationQueue"/>

    <bean id="migrationMessageSender" class="com.aplana.sbrf.taxaccounting.migration.service.MessageService">
        <property name="connectionFactory" ref="migrationConnectionFactory"/>
        <property name="queue" ref="migrationQueue"/>
    </bean>

</beans>
