       xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
		">
    <import resource="classpath:testServiceContext.xml"/>

    <bean class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aplana.sbrf.taxaccounting.service.FormTemplateService"/>
    </bean>
    <bean class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aplana.sbrf.taxaccounting.service.FormDataService"/>
