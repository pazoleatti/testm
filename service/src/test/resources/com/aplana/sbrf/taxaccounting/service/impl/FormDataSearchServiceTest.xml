<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		">
    <import resource="classpath:testServiceContext.xml"/>

    <bean class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aplana.sbrf.taxaccounting.service.FormTemplateService"/>
    </bean>
    <bean class="org.mockito.Mockito" factory-method="mock">
        <constructor-arg value="com.aplana.sbrf.taxaccounting.service.FormTypeService"/>
